{
  "name": "gabyquiles/auth-bundle",
  "version": "1.2.2",
  "license": "Apache-2.0",
  "keywords": [
    "aws",
    "cognito",
    "authentication"
  ],
  "description": "Provide the verification for jwt from AWS Cognito",
  "type": "library",
  "authors": [
    {
      "name": "Gabriel A. Quiles-Perez",
      "email": "info@gabyquiles.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "symfony/framework-bundle": "^3.4|^4.0|^6.1",
    "symfony/security-bundle": "^3.4|^4.0|^6.1",
    "lexik/jwt-authentication-bundle": "^2.12",
    "codercat/jwk-to-pem": "^1.1",
    "symfony/http-client": "^4.4|^6.1"
  },
  "require-dev": {
    "symfony/phpunit-bridge": "^3.4|^4.0|^6.1",
    "symfony/browser-kit": "^3.4|^4.0|^6.1",
    "symfony/console": "^3.4|^4.0|^6.1",
    "symfony/dom-crawler": "^3.4|^4.0|^6.1",
    "symfony/var-dumper": "^3.4|^4.0|^6.1",
    "symfony/yaml": "^3.4|^4.0|^6.1",
    "friendsofphp/php-cs-fixer": "^1.1|^2.8|^3.12",
    "ext-json": "*",
    "nyholm/symfony-bundle-test": "^2.0",
    "matthiasnoback/symfony-dependency-injection-test": "^4.2"
  },
  "autoload": {
    "psr-4": {
      "GabyQuiles\\Auth\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GabyQuiles\\Auth\\Test\\": "tests/"
    }
  }
}
